+  
+  try {
+    let content = fs.readFileSync(filePath, 'utf-8');
+    let updated = false;
+    
+    // Remove Image components with empty src
+    content = content.replace(
+      /<Image[^>]*src=\{\}[^>]*\/?>/g,
+      (match) => {
+        updated = true;
+        console.log(`  🗑️  Removed Image with empty src: ${match.trim()}`);
+        return '';
