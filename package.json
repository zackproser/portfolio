{
  "name": "zackproser-portfolio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "concurrently \"next dev\" \"pnpm stripe:webhook\"",
    "build": "prisma generate && (prisma migrate deploy || echo 'Database migration failed, continuing with build...') && NODE_OPTIONS=--max-old-space-size=6144 next build",
    "build-no-db": "npm run prebuild && prisma generate && NODE_OPTIONS=--max-old-space-size=6144 next build",
    "build-with-tests": "npm run test && npm run prebuild && prisma generate && prisma migrate deploy && NODE_OPTIONS=--max-old-space-size=6144 next build",
    "build-only": "prisma generate && NODE_OPTIONS=--max-old-space-size=6144 next build",
    "start": "next start",
    "lint": "next lint",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "grant-access": "tsx scripts/grant-free-access.ts",
    "test": "NODE_ENV=development jest",
    "test:watch": "NODE_ENV=development jest --watch",
    "test:coverage": "NODE_ENV=development jest --coverage",
    "ci": "NODE_ENV=development jest && npm run lint",
    "postinstall": "prisma generate",
    "og:generate": "tsx scripts/og/og-image-generator.ts --verbose",
    "og:generate-for": "tsx scripts/og/og-image-generator.ts",
    "og:clean": "rm -rf public/og-images/*.png",
    "og:clean-orphans": "tsx --tsconfig scripts/tsconfig.json scripts/clean-og-images.ts",
    "og:optimize": "node scripts/optimize-og-images.js",
    "og:optimize-replace": "node scripts/optimize-og-images.js --replace",
    "og:migrate": "node scripts/og/migrate-og-images-to-cdn.js",
    "prisma:seed": "tsx --tsconfig prisma/tsconfig.json prisma/seed.ts",
    "prisma:seed-production": "./scripts/seed-production.sh",
    "knip": "knip"
  },
  "browserslist": "defaults, not ie <= 11",
